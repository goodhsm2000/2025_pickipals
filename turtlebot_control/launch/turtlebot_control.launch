<launch>
    <!-- Node for controlling the LED based on button input -->
    <node name="button_led_control" pkg="turtlebot_control" type="button_led_control.py" output="screen">
        <param name="use_sim_time" value="false"/>
    </node>


    <!-- Include the keyboard teleop launch file -->
    <include file="$(find turtlebot_teleop)/launch/keyboard_teleop.launch" />
</launch>code



